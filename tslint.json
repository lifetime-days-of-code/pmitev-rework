{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    },
    "rules": {
        "max-line-length": {
            "options": [120],
            "no-console": {
                "severity": "warning",
                "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
            }
        }
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": ["assets/public/main.js"]
    }
}